  <div class="top_space_nav"></div>
  <img class="contact_header_img" width="1200px" height="600px" src="/img/header_footer.jpg" alt="header_img"/>
<div class="register_table">
  <form action="/register_check" method="post" onsubmit="return conrimMessage();">
        <h1>アカウントの作成</h1>
  <table>
    <tr>
      <td><p>使用したいID</p></td>
      <td><input type="text" name="id" id="id" placeholder="ユーザID" required aria-required="true" ></td>
    </tr>
    <tr>
      <td><p>ユーザーネーム</p></td>
      <td><input type="text" name="user_name" id="id" placeholder="ユーザネーム" required aria-required="true" ></td>
    </tr>
    <tr>
      <td><p>性別</p></td>
      <td><span>男&nbsp;<input type="radio" name="user_sex" id="id" value="0"></span><span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;女&nbsp;<input type="radio" name="user_sex" id="id" value="1"></span>
      </td>
    </tr>
    <tr>
      <td><p>年齢</p></td>
      <td>
        <select name="year">
        <option value="" selected>年代</option>
        <option value="10代">10代</option>
        <option value="20代">20代</option>
        <option value="30代">30代</option>
        <option value="40代">40代</option>
        <option value="50代">50代</option>
        <option value="60代">60代</option>
        </select>
      </td>
    </tr>
    <tr>
      <td><p>職業</p></td>
      <td><input type="text" name="user_job" id="id" placeholder="職業"></td>
    </tr>
    <tr>
      <td><p>得意な言語</p></td>
      <td><input type="text" name="user_language" id="id" placeholder="言語名"></td>
    </tr>
    <tr>
      <td><p>Emailアドレス</p></td>
      <td><input type="email" name="Email" id="Email" size="20" maxlength="40" placeholder="メールアドレスを入力" required aria-required="true" ></td>
    </tr>
    <tr>
      <td><p>メールアドレスの確認</p></td>
      <td><input type="email" name="confirm_Email" id="confirm_Email" size="20" maxlength="40" placeholder="メールアドレスを再度入力" onkeyup=setConfirmMessage1(this.value); required aria-required="true" ></td>
      <div id="Email_confirm_message"></div>
    </tr>
    <tr>
      <td><p>パスワードの設定</p></td>
      <td><input type="password" name="password" placeholder= "パスワードを入力" id="password" onkeyup="setMessage(this.value);" required aria-required="true" ></td>
      <td><div id="pass_message"></div></td>
    </tr>
    <tr>
    <td><p></p></td>
    <td><input type="password" name="confirm_password" placeholder= "再度パスワードを入力" id="confirm_password" onkeyup="setConfirmMessage(this.value);" required aria-required="true" ></td>
      <div id="pass_confirm_message"></div>
    </tr>
  </table>
  <div class="register_input">
    <input type="submit" value="登録">
    <input type="reset" value="リセット">
    <input id="token" type="hidden" name="_csrf" value="<%= reqCsrf %>"><!-- csrfToken -->
  </div>
     <p class="center-text"><a href="/password_reset">パスワードリセット</a></p>
  <input id="Error_status" type="hidden" name="Error_status" value="<%= error %>">
  </form>
</div>
  </body>

</html>
